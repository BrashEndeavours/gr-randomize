<?xml version="1.0"?>
<block>
  <name>Constant Complex Offset</name>
  <key>randomize_const_offset_cc</key>
  <category>[Thesis]/Randomize</category>
  <import>import randomize</import>
  <make>randomize.const_offset_cc()</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->


  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
